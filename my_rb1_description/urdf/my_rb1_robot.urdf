<?xml version="1.0" ?>
<robot name="rb1_robot">
    <!-- Define materials for visualization -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>
    <material name="gray">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>
    
    <!-- Base footprint link -->
    <link name="base_footprint">
    </link>
    
    <!-- Joint connecting base_link and base_footprint -->
    <joint name="base_link_plate_joint" type="fixed">
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
        <parent link="base_footprint"/>
        <child link="base_link"/>
    </joint>
    
    <!-- Base link -->
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="blue"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="19"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.4277" ixy="0" ixz="0" iyy="0.4277" iyz="0" izz="0.5781"/>
        </inertial>
    </link>
    
    <!-- Left wheel link -->
    <link name="left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
            <material name="gray"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.000365" ixy="0" ixz="0" iyy="0.000365" iyz="0" izz="0.0003125"/>
        </inertial>
    </link>
    
    <!-- Joint connecting base_link and left_wheel -->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <axis xyz="0 1 0"/>
        <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="left_wheel"/>
    </joint>
    
    <!-- Right wheel link -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
            <material name="gray"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.05"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.000365" ixy="0" ixz="0" iyy="0.000365" iyz="0" izz="0.0003125"/>
        </inertial>
    </link>
    
    <!-- Joint connecting base_link and right_wheel -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <axis xyz="0 1 0"/>
        <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="right_wheel"/>
    </joint>
    
    <!-- Front caster -->
    <link name="front_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="gray"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>
                        <mu2>0.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <inertial>
            <mass value="1.5"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.000375" ixy="0" ixz="0" iyy="0.000375" iyz="0" izz="0.000375"/>
        </inertial>
    </link>
    
    <!-- Joint connecting base_link and front_caster -->
    <joint name="base_link_front_caster_joint" type="fixed">
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="front_caster"/>
    </joint>
    
    <!-- Back caster -->
    <link name="back_caster">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="gray"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>
                        <mu2>0.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
        
        <inertial>
            <mass value="1.5"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.000375" ixy="0" ixz="0" iyy="0.000375" iyz="0" izz="0.000375"/>
        </inertial>
    </link>
    
    <!-- Joint connecting base_link and back_caster -->
    <joint name="base_link_back_caster_joint" type="fixed">
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="back_caster"/>
    </joint>
    
    <!-- Link representing the front laser scanner -->
    <link name="front_laser">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.03" length="0.04"/>
            </geometry>
            <material name="red"/>
        </visual>
        
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.03" length="0.04"/>
            </geometry>
        </collision>
        
        <inertial>
            <mass value="1.0"/>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <inertia ixx="0.000308" ixy="0" ixz="0" iyy="0.000308" iyz="0" izz="0.00045"/>
        </inertial>
    </link>
    
    <!-- Joint connecting base_link and front_laser -->
    <joint name="base_link_front_laser_joint" type="fixed">
        <origin xyz="0.25 0.0 0.075" rpy="3.14159 0 0"/>
        <parent link="base_link"/>
        <child link="front_laser"/>
    </joint>
</robot>